<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:ZipPicViewUWP">

    <Color x:Key="PrimaryColor1">#FFAAAA</Color>
    <Color x:Key="PrimaryColor2">#D46A6A</Color>
    <Color x:Key="PrimaryColor3">#AA3939</Color>
    <Color x:Key="PrimaryColor4">#801515</Color>
    <Color x:Key="PrimaryColor5">#550000</Color>

    <SolidColorBrush x:Key="SidebarBackground" Color="{StaticResource PrimaryColor5}"/>
    <SolidColorBrush x:Key="FloatControlBackground" Color="#33000000"/>

    <SolidColorBrush x:Key="SystemControlHighlightAccentBrush" Color="{ThemeResource PrimaryColor3}"/>
    <SolidColorBrush x:Key="SystemControlHighlightListAccentLowBrush" Color="{ThemeResource PrimaryColor3}"/>
    <SolidColorBrush x:Key="SystemControlHighlightListAccentMediumBrush" Color="{ThemeResource PrimaryColor2}"/>
    <SolidColorBrush x:Key="SystemControlHighlightListAccentHighBrush" Color="{ThemeResource PrimaryColor1}"/>
    
</ResourceDictionary>
