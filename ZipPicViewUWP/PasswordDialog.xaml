<ContentDialog
    x:Class="ZipPicViewUWP.PasswordDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Title="Password"
    PrimaryButtonText="Ok">

    <StackPanel Height="90" VerticalAlignment="Top" Padding="0" Width="342">
        <TextBlock HorizontalAlignment="Left" Margin="10,9,0,0" TextWrapping="Wrap" Text="The input file is possibly encrypted. Please enter the password." VerticalAlignment="Top"/>
        <PasswordBox x:Name="passwordBox" Margin="10,8,10,0" VerticalAlignment="Top"/>
    </StackPanel>

</ContentDialog>
